<swiper indicator-dots="{{indicatorDots}}"  
    autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" indicator-color="#fff" indicator-active-color="#3c9ae8">  
    <block wx:for="{{imgUrls}}">  
      <swiper-item>  
        <image src="{{item}}" class="slide-image" />  
      </swiper-item>  
    </block>  
</swiper>  